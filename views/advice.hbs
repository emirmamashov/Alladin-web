<script>
    var popupSbscShow = false;
    var popupSbscDays = 30;
    var popupSbscTime = 2;
</script>

<script type="text/javascript">
    $(function () {
        var pageStep = $.cookie('pageStep') | 1;
        $.cookie('_pageStep', pageStep + 1, {path: '/', domain: window.lmSettings.cookieDomain});

        $('#subscribe-btn').on('click', function () {
            var form = $('.subscribe-new-form');
            $.post(form.attr('action'),
                    form.serialize(),
                    function (json) {
                        var msg = '<h2 class="subscribe-new__hl">Благодарим Вас за подписку на рассылку.<br> Для подтверждения подписки, Вам нужно перейти по ссылке в письме.</h2>';

                        if (json.error) {
                            if (json.subscribed) {
                                msg = '<h2 class="subscribe-new__hl">Вы уже подписаны на рассылку!</h2>';
                            } else {
                                msg = json.message ? json.message : 'Некорректный e-mail.';
                                form.find('input[type="email"]').addClass('error');
                                if (!form.find('.error-form').length) {
                                    form.find('input[type="email"]').after('<p class="error-form">' + msg + '</p>');
                                    setTimeout(function () {
                                        form.find('.error-form').remove();
                                    }, 3000);
                                }
                                return;
                            }
                        }

                        $('#popup-subscribe-body')
                                .empty()
                                .css('text-align', 'center')
                                .html(msg);

                        setTimeout(function () {
                            $('.js-subscribe-new').slideUp();
                        }, 3000);
                    }
            );

            return false;
        });

        $('#popup-subscribe-btn').on('click', function () {
            var form = $('.subscribe__form_short');

            $.post(form.attr('action'),
                    form.serialize(),
                    function (json) {
                        if (json.error) {
                            form.find('.error-form').show();
                            form.find('input[type="email"]').addClass('error');
                            return;
                        }

                        popups.open('#popup-success');
                    }
            );

            return false;
        });

        $('.popup-success__close').on('click', function () {
            popups.close();

            return false;
        });
    });

    $(window).load(function () {
        if ($('#popup-subscribe_short').length && $.cookie('_cookie_accepted')) {
            if (!popupSbscTime) {
                popupSbscTime = 2000;
            }

            setTimeout(function () {
                popups.open('#popup-subscribe_short');
            }, popupSbscTime);

            popupSbscDays = popupSbscDays ? popupSbscDays : 30;
            $.cookie('_SBSShown', true, {expires: popupSbscDays, path: '/', domain: window.lmSettings.cookieDomain});
        }
    });
</script>
