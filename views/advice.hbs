        function hideNotsearch(inp_search, len) {

            if (inp_search.length <= 0 || inp_search.length <= len) {
                $(".myshop-popup__block .myshop-popup__item").removeClass('ng-hide');
                return false;
            }
            console.log(inp_search)
            if (inp_search.length > len) {
                var arData = $(".myshop-popup__block .myshop-popup__item");
                //$(arData).removeClass('ng-hide');
                if (arData.length <= 0) return false;

                for (var index = 0; index < arData.length; index++) {
                    var name = $(arData[index]).find("span[data-type=name]").text();
                    var addr = $(arData[index]).find("p[data-type=addr]").text();
                    console.log(name)
                    console.log(addr)
                    if (name.toLowerCase().indexOf(inp_search.toLowerCase()) < 0 && addr.toLowerCase().indexOf(inp_search.toLowerCase()) < 0) $(arData[index]).addClass('ng-hide');
                    else $(arData[index]).removeClass('ng-hide');
                }
            }
        }
    </script>

