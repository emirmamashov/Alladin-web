<script>

    $(".catalog__fast-view").click(function(){
        $(".modal-popup-wrap").fadeIn('fast');
        $(".modal-popup").fadeIn('fast');
    });

    $(".modal-popup-wrap").click(function(e){
        if (e.target.classList.contains("modal-popup-wrap")) {
            $(".modal-popup").fadeOut('fast');
            $(this).fadeOut('fast');
        }
    });

    function openModulProductDetails(product) {
        console.log(product);
        if (product.name) {
            $('#name')[0].innerText = product.name;
        }
        if (product.description) {
            $('#description')[0].innerHTML = product.description;
        }
        if (product.price) {
            $('.product__price')[0].innerText = product.price;
        }
        if (product.id) {
            $('.product__article')[0].innerText = '(Артикл: ' + product.id + ')';
        }
        if (product.image) {
            $('#productImage')[0].src = product.image;
            $('#productImageGallery')[0].src = product.image;
        }
    }

</script>